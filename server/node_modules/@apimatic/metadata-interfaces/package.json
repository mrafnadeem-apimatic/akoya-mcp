{
  "name": "@apimatic/metadata-interfaces",
  "author": "APIMatic Ltd.",
  "version": "0.1.1-alpha.1",
  "license": "MIT",
  "sideEffects": false,
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "umd",
    "lib/**/*.js",
    "lib/**/*.d.ts",
    "es/**/*.js",
    "es/**/*.d.ts",
    "src",
    "LICENSE.md"
  ],
  "engines": {
    "node": ">=14.15.0 || >=16.0.0"
  },
  "scripts": {
    "clean": "rimraf lib es umd tsconfig.tsbuildinfo",
    "test": "jest --ci --coverage --maxWorkers=2 --passWithNoTests",
    "build": "npm run clean && tsc && rollup -c --bundleConfigAsCjs && npm run annotate:es",
    "annotate:es": "babel es --out-dir es --no-babelrc --plugins annotate-pure-calls",
    "preversion": "npm run test",
    "prepublishOnly": "npm run build",
    "lint": "tslint --project .",
    "lint:fix": "tslint --project . --fix",
    "check-style": "prettier --check \"{src,test}/**/*.ts\"",
    "check-style:fix": "prettier --write \"{src,test}/**/*.ts\""
  },
  "size-limit": [
    {
      "path": "es/index.js",
      "limit": "5 KB"
    }
  ],
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.10",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "jest": "^26.6.3",
    "jsdom": "^19.0.0",
    "jsdom-global": "^3.0.2",
    "lerna-alias": "3.0.3-0",
    "rollup": "^3.29.5",
    "rollup-plugin-typescript2": "^0.31.0",
    "ts-jest": "^26.4.0",
    "typescript": "^5.2.0"
  },
  "dependencies": {
    "@apimatic/core-interfaces": "^0.2.13-alpha.1",
    "@apimatic/schema": "^0.7.21-alpha.2",
    "tslib": "^2.8.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:apimatic/apimatic-js-runtime.git",
    "directory": "packages/metadata-interfaces"
  },
  "gitHead": "5b639da0d0f98fce6d17ea17068d7122165a17d1"
}
